package StringConcept;

import java.awt.List;
import java.util.ArrayList;



public class CountStringandInteger {

	public static void main(String[] args) {
		String letter = "nAJh48379gk";
		int integercount=0;
		int lettercount=0;
		ArrayList<Character> al= new ArrayList<Character>();
		ArrayList<Character> aa= new ArrayList<Character>();

		
		for(int i=0;i<letter.length();i++) {
			    char charAt = letter.charAt(i);
			
			if(Character.isDigit(charAt)) {
				integercount++;
				al.add(charAt);
			}
			else if(Character.isLetter(charAt)) {
				lettercount++;
				aa.add(charAt);
			}
				
		}
		System.out.println("Integer Count is "+integercount+" they are ");
		for (Character integer : al) {
			System.out.print(integer+" ");
		}
		System.out.println();
		System.out.println("letter Count is "+lettercount+" they are ");
		for (Character letters : aa) {
			System.out.print(letters+" ");
		}
		

		
		

		
	}
}




